<UserControl x:Class="Examen2doParcial.ControlContacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Examen2doParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="500">
    <StackPanel Background="White">

        <TextBlock>Correo</TextBlock>
        <TextBox Name="txtCorreo"></TextBox>
        <TextBlock>Telefono</TextBlock>
        <TextBox Name="txtTelefono"></TextBox>
        <TextBlock>Celular</TextBlock>
        <TextBox Name="txtCelular"></TextBox>

    </StackPanel>
</UserControl>
